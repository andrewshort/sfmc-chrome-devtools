{
	"name": "Salesforce Marketing Cloud - Developer Tools",
	"short_name": "SFMC Devtools",
	"version": "0.1.1",
	"description": "An extension that makes life easier for Salesforce Marketing Cloud users.",
	"author": "Markus Slabina <me@markus-slabina.at>",
	"devtools_page": "devtools.html",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": "icons/icon128.png",
		"default_title": "Salesforce Marketing Cloud - Developer Tools",
		"default_popup": "browser_action.html"
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"background": {
		"scripts": [
			"scripts/background.js"
		],
		"persistent": false
	},
	"web_accessible_resources": [
		"scripts/passPubListData.js"
	],
	"manifest_version": 2
}
